<%- include('partials/head',{}) %>

<div id="messages">

</div>

<div class="container">

<div id="game">
	<div id="infinitive"></div>
	<form class="form-horizontal game-form">
		<div class="form-group">
			<div class="col-sm-3">
				<label class="control-label" id="conjugationtype" for="conjugation"></label>
			</div>
			<div class="col-sm-6">
				<input type="text" name="conjugation" id="conjugation" class="form-control input-lg" placeholder="Conjugated verb" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false">
			</div>
			<div class="col-sm-3">
				<button class="btn btn-warning btn-block btn-lg" id="gamecheck">Check</button>
			</div>
		</div>
		
	</form>
	<div class="answer correct">Correct</div>
	<div class="answer incorrect">Incorrect</div>
</div>
</div>

<%- include('partials/footer',{}) %>